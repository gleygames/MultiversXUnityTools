{
  "apiName": "MultiversXApiMainnet",
  "baseAddress": "https://api.elrond.com",
  "endpoints": [
    {
      "name": "GetNetworkConfig",
      "baseAddress": "https://api.elrond.com",
      "resourceName": "network/config"
    },
    {
      "name": "GetAccount",
      "baseAddress": "https://api.elrond.com",
      "resourceName": "address/{address}"
    },
    {
      "name": "SendTransaction",
      "baseAddress": "https://api.elrond.com",
      "resourceName": "transaction/send"
    },
    {
      "name": "GetTransactionCost",
      "baseAddress": "https://api.elrond.com",
      "resourceName": "transaction/cost"
    },
    {
      "name": "QueryVm",
      "baseAddress": "https://api.elrond.com",
      "resourceName": "vm-values/query"
    },
    {
      "name": "GetTransactionDetail",
      "baseAddress": "https://api.elrond.com",
      "resourceName": "transactions/{txHash}"
    },
    {
      "name": "GetWalletNfts",
      "baseAddress": "https://api.elrond.com",
      "resourceName": "accounts/{address}/nfts?from={start}&size={totalNfts}"
    },
    {
      "name": "GetWalletTokens",
      "baseAddress": "https://api.elrond.com",
      "resourceName": "accounts/{address}/tokens?from={start}&size={totalTokens}"
    },
    {
      "name": "GetTokensCount",
      "baseAddress": "https://api.elrond.com",
      "resourceName": "accounts/{address}/tokens/count"
    },
    {
      "name": "GetNFTsCount",
      "baseAddress": "https://api.elrond.com",
      "resourceName": "accounts/{address}/nfts/count"
    }
  ]
}